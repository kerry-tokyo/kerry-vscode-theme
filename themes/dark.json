  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#6a737d"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#f0d162"
      }
    },
    {
      "scope": ["entity", "entity.name"],
      "settings": {
        "foreground": "#b392f0"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#ccc"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#70be86"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#f06292"
      }
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#f06292"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#ccc"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#70beb9"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#f0d162"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#f0d162"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#f08d62"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#ccc"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f06292"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f06292"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f06292"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f06292"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#f06292",
        "foreground": "#1C232E",
        "content": "^M"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#f06292"
      }
    },
    {
      "scope": "string source",
      "settings": {
        "foreground": "#ccc"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#f0d162"
      }
    },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": {
        "foreground": "#70beb9"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#70beb9"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#70be86"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#f0d162"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#f0d162"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#f0d162"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#f08d62"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f0d162"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#70be86"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ccc"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ccc"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#f0d162"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#86181d",
        "foreground": "#f06292"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#144620",
        "foreground": "#70be86"
      }
    },
    {
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {
        "background": "#c24e00",
        "foreground": "#f08d62"
      }
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "foreground": "#2f363d",
        "background": "#f0d162"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#b392f0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#f0d162"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f0d162"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#f0d162"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#d1d5da"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#f06292"
      }
    },
    {
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": {
        "foreground": "#70beb9",
        "fontStyle": "underline"
      }
    }
  ]
}
